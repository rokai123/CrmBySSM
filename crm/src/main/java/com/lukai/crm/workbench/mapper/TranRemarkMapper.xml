<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.lukai.crm.workbench.mapper.TranRemarkMapper">

    <!-- ==================== ResultMap ==================== -->
    <resultMap id="BaseResultMap" type="com.lukai.crm.workbench.domain.TranRemark">
        <id     column="id"          property="id" jdbcType="CHAR"/>
        <result column="note_content" property="noteContent"/>
        <result column="create_by"    property="createBy"/>
        <result column="create_time"  property="createTime"/>
        <result column="edit_by"      property="editBy"/>
        <result column="edit_time"    property="editTime"/>
        <result column="edit_flag"    property="editFlag"/>
        <result column="tran_id"      property="tranId"/>
    </resultMap>

    <!-- ==================== Base Column List ==================== -->
    <sql id="Base_Column_List">
      	id,
        note_content,
        create_by,
        create_time,
        edit_by,
        edit_time,
        edit_flag,
        tran_id
    </sql>

   <insert id="insertTranRemarkByList"
        parameterType="java.util.List">
    INSERT INTO tbl_tran_remark (
        id,
        note_content,
        create_by,
        create_time,
        edit_by,
        edit_time,
        edit_flag,
        tran_id
    )
    VALUES
    <foreach collection="list" item="tr" separator=",">
        (
            #{tr.id},
            #{tr.noteContent},
            #{tr.createBy},
            #{tr.createTime},
            #{tr.editBy},
            #{tr.editTime},
            #{tr.editFlag},
            #{tr.tranId}
        )
    </foreach>
</insert>
   

</mapper>
